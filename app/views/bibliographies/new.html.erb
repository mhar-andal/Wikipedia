<h3>Bibliography</h3>
<%= form_for :bibliography, url: article_bibliographies_path(@article.id) do |f| %>
  
  <% if @bib.errors.any? %>
    <%= render partial: 'errors/errors', locals: { errors: @bib } %>
  <% end %>
  <%= render partial: "bibliographies/new_bibliography", locals: {f: f}%>
   <p>
    <%= f.submit "Create Bibliography" %>
  </p>

<% end %>